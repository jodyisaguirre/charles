var arr = [{"name": "spencer"},{"name": "nmuta"},{"name": "sean"}]
var bigger = [{"name": "spencer","occupation" : "resident instructor"},{"name": "nmuta","occupation" : "lead instructor"},{"name": "sean","occupation" : "instructor"}]

for (i = 0; i < arr.length; i++){
  let main = document.queryselector("#myDiv")
  let li = document.createElement ("li")
  main.appendChild(li)
  li.innerHTML = arr[i].name
}

for (i = 0; i < arr.length; i++){
  let main = document. queryselector("#myDiv")
  let h3 = document.createElement ("h3")
  let p = document.createElement ("p")
  main.appendChild(h3)
  main.appendChild(p)
  h3.innerHTML = bigger[i].name
  h3.innerHTML = bigger[i].occupation
  p.setAttribute ("class","paragraph")
  h3.style.color = "blue"
  p.style.backgroudColor = "orange"
  img.src = ""
}
document.queryselectorAll('p')


